    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT TimeCategoryExpressionResult1.DISPLAY_SEQUENCE, 
       TimeCategoryExpressionResult1.DISPLAY_VALUE, 
       TimeCategoryExpressionResult1.EXPR_ROW_IDENTIFIER, 
       TimeCategoryExpressionResult1.EXPRESSION_GROUP_ID, 
       TimeCategoryExpressionResult1.LEFT_PARENTHESIS_NUM, 
       TimeCategoryExpressionResult1.OPERATOR AS OPERATOR11, 
       TimeCategoryExpressionResult1.RIGHT_PARENTHESIS_NUM, 
       TimeCategoryExpressionResult1.TCAT_EXPR_RESULT_ID, 
       TimeCategoryExpressionResult1.TCAT_ID, 
       TimeCategoryExpressionResult1.TIME_ATRB_FLD_ID, 
       TimeCategoryExpressionResult1.USER_DESCRIPTION, 
       TimeCategoryExpressionResult1.VALUE_CHAR, 
       TimeCategoryExpressionResult1.VALUE_DATE, 
       TimeCategoryExpressionResult1.VALUE_ID, 
       TimeCategoryExpressionResult1.VALUE_TIMESTAMP, 
       TimeCategoryExpressionResult1.VALUE_TYPE, 
       TimeCategoryExpressionResult1.OBJECT_VERSION_NUMBER, 
       TimeAttributeFieldPEO.ATTRIBUTE_TYPE, 
       TimeAttributeFieldPEO.NAME, 
       TimeAttributeFieldPEO.TM_ATRB_FLD_ID, 
       TimeCategoriesEO.TCAT_CD, 
       TimeCategoriesEO.TCAT_ID AS TCAT_ID1, 
       TimeCategoriesEO.OBJECT_VERSION_NUMBER AS OBJECT_VERSION_NUMBER1, 
       DataSourcesPEO.DATA_SOURCE_CODE, 
       DataSourcesPEO.DATA_SOURCE_ID, 
       TimeCategoriesEO.PERSISTENT_FLAG
FROM HWM_TCAT_EXPR_RESULTS_VL TimeCategoryExpressionResult1, HWM_TM_ATRB_FLDS_VL TimeAttributeFieldPEO, HWM_TCATS_VL TimeCategoriesEO, HWM_DATA_SOURCE_USAGES DataSourceUsagesPEO, HWM_DATA_SOURCES_VL DataSourcesPEO
WHERE TimeCategoryExpressionResult1.TIME_ATRB_FLD_ID  = TimeAttributeFieldPEO.TM_ATRB_FLD_ID
AND TimeCategoryExpressionResult1.TCAT_ID = TimeCategoriesEO.TCAT_ID
AND TimeCategoryExpressionResult1.DISPLAY_SEQUENCE != 0
AND DataSourceUsagesPEO.TM_ATRB_FLD_ID(+) = TimeAttributeFieldPEO.TM_ATRB_FLD_ID
AND DataSourceUsagesPEO.USAGE_TYPE(+) LIKE '%ADMIN%'
AND DataSourceUsagesPEO.DEFAULT_FLAG(+) = 'Y'
AND DataSourceUsagesPEO.DATA_SOURCE_ID = DataSourcesPEO.DATA_SOURCE_ID(+)
ORDER BY TimeCategoryExpressionResult1.DISPLAY_SEQUENCE]]>
  </SQLQuery>
